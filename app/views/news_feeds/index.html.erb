<h1>Listing news_feeds</h1>

<table>
  <tr>
    <th>Host</th>
    <th>Selector</th>
    <th>Url pattern</th>
    <th>Title</th>
    <th>Subtitle</th>
    <th>Image source</th>
    <th>Date tag</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @news_feeds.each do |news_feed| %>
  <tr>
    <td><%= news_feed.host %></td>
    <td><%= news_feed.selector %></td>
    <td><%= news_feed.url_pattern %></td>
    <td><%= news_feed.title %></td>
    <td><%= news_feed.subtitle %></td>
    <td><%= news_feed.image_source %></td>
    <td><%= news_feed.date_tag %></td>
    <td><%= link_to 'Show', news_feed %></td>
    <td><%= link_to 'Edit', edit_news_feed_path(news_feed) %></td>
    <td><%= link_to 'Destroy', news_feed, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New News feed', new_news_feed_path %>
